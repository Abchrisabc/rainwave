<div bind="el" class="timeline_event timeline_{{ type }}">
	<div class="timeline_header">
		<span class="timeline_header_clock" bind="clock"></span>
		<a bind="header"></a>
	</div>

	{{#each songs }}
		{{>song}}
	{{/each}}
</div>

<div class="timeline_header_bar">
	<div bind="progress" class="timeline_header_bar_inside"></div>
</div>